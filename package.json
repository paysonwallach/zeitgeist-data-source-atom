{
  "name": "zeitgeist",
  "version": "0.2.0",
  "description": "Log activity in Atom to Zeitgeist.",
  "license": "GPL-3.0",
  "author": "Payson Wallach <payson@paysonwallach.com>",
  "homepage": "https://github.com/paysonwallach/atom-zeitgeist#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/paysonwallach/atom-zeitgeist"
  },
  "main": "dist/main",
  "scripts": {
    "build": "run-s clean build:ts",
    "build:ts": "tsc --outDir dist",
    "clean": "shx rm -rf dist",
    "format": "run-p format:*",
    "format:js": "eslint --fix .",
    "format:package.json": "prettier-package-json --write",
    "prerelease": "scrivano",
    "release": "standard-version"
  },
  "dependencies": {
    "file-url": "^3.0.0",
    "mmmagic": "^0.5.3",
    "tslib": "^2.0.3",
    "typescript-json-serializer": "^2.5.2",
    "uuidv4": "^6.2.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/atom": "^1.40.5",
    "@types/electron": "^1.6.10",
    "@types/mmmagic": "^0.4.30",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-package-json": "^2.1.3",
    "scrivano": "^0.1.0",
    "shx": "^0.3.3",
    "standard-version": "^9.1.0",
    "typescript": "^4.1.3"
  },
  "keywords": [
    "activity tracking",
    "time tracking",
    "zeitgeist"
  ],
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run format"
    }
  },
  "meta": {
    "title": "atom-zeitgeist",
    "repository": {
      "owner": "paysonwallach",
      "name": "atom-zeitgeist"
    },
    "license": {
      "name": "GNU General Public License v3.0",
      "url": "https://github.com/paysonwallach/atom-zeitgeist/blob/master/LICENSE"
    },
    "codeOfConduct": {
      "url": "https://github.com/paysonwallach/atom-zeitgeist/blob/master/CODE_OF_CONDUCT.md"
    },
    "releases": {
      "url": "https://github.com/paysonwallach/atom-zeitgeist/releases"
    }
  },
  "standard-version": {
    "scripts": {
      "precommit": "npm run prerelease"
    }
  }
}
